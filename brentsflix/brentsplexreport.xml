<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Name>plexReport</Name>
  <Description>This is a Dockerfile for \"plexReport\" - (https://github.com/bstascavage/plexReport)[br][br]&#13;
[b][span style=\'color: #E80000;\']Directions:[/span][/b][br]&#13;
[b]/config[/b] : Where the config file will be created.[br]&#13;
</Description>
  <Registry>https://registry.hub.docker.com/u/ninthwalker/brentsplexreport/</Registry>
  <Repository>ninthwalker/brentsplexreport</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Environment/>
  <Networking>
    <Mode>host</Mode>
    <Publish/>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/cache/.Programs/plexreport/</HostDir>
      <ContainerDir>/config</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/etc/localtime</HostDir>
      <ContainerDir>/etc/localtime</ContainerDir>
      <Mode>ro</Mode>
    </Volume>
  </Data>
  <Version></Version>
  <WebUI></WebUI>
  <Banner></Banner>
  <Icon>http://www.macupdate.com/util/iconlg/27302.png</Icon>
  <ExtraParams></ExtraParams>
</Container>
